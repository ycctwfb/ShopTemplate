{% doc %}
  @prompt
    Create a full-viewport luxury hero section for a Shopify product page selling a Rose Teddy Bear.
    
    Background: soft pale blush-pink gradient with slow-floating translucent love hearts drifting upward, subtle parallax movement on scroll.
    
    Centrepiece: the Rose Teddy Bear product image placed perfectly centre, softly glowing with a warm romantic rim light.
    
    Add micro-animations on hover:
    – product gently lifts and rotates 2–3 degrees
    – hearts subtly pulse outward from the product
    – soft light bloom intensifies briefly
    
    Typography: elegant high-end serif headline with a modern sans-serif subheading.
    
    Headline example: “More Than a Gift. A Memory.”
    Subtext: emotionally charged but minimal, focused on romance and permanence.
    
    Primary CTA: “Buy For Valentine’s Day” in a rounded luxury button with soft glow and heart pulse animation on hover.
    
    Secondary CTA: “See Why Everyone Loves It” scrolling to social proof section.
    
    Overall style: premium jewellery / luxury perfume campaign, no clutter, no harsh pinks, no gimmicks.
    
    Mobile-first optimised, fast-loading, Shopify-safe animations only., there should be a love heart pulsating around the product, make the love heart biiger, make the love heart animation when hovering over the product image more intensne big and glowing 
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-luxury-hero-{{ ai_gen_id }} {
    display: block;
    position: relative;
    width: 100%;
    min-height: 100vh;
    overflow: hidden;
    background: linear-gradient(
      135deg,
      {{ block.settings.gradient_color_1 }} 0%,
      {{ block.settings.gradient_color_2 }} 50%,
      {{ block.settings.gradient_color_3 }} 100%
    );
  }

  .ai-luxury-hero__container-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    height: 100vh;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding: 40px 20px;
    z-index: 2;
  }

  .ai-luxury-hero__hearts-canvas-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    pointer-events: none;
    z-index: 1;
  }

  .ai-luxury-hero__heart-{{ ai_gen_id }} {
    position: absolute;
    opacity: 0;
    pointer-events: none;
    filter: blur(1px);
  }

  .ai-luxury-hero__content-{{ ai_gen_id }} {
    max-width: 1200px;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    text-align: center;
    gap: 40px;
  }

  .ai-luxury-hero__product-wrapper-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    max-width: 500px;
    transition: transform 0.6s cubic-bezier(0.34, 1.56, 0.64, 1);
  }

  .ai-luxury-hero__product-wrapper-{{ ai_gen_id }}:hover {
    transform: translateY(-12px) rotate({{ block.settings.hover_rotation }}deg);
  }

  .ai-luxury-hero__product-glow-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 100%;
    height: 100%;
    border-radius: 50%;
    background: radial-gradient(
      circle,
      {{ block.settings.glow_color }} 0%,
      transparent 70%
    );
    opacity: {{ block.settings.glow_opacity }};
    filter: blur(40px);
    transition: opacity 0.6s ease, filter 0.6s ease;
    pointer-events: none;
  }

  .ai-luxury-hero__product-wrapper-{{ ai_gen_id }}:hover .ai-luxury-hero__product-glow-{{ ai_gen_id }} {
    opacity: {{ block.settings.glow_hover_opacity }};
    filter: blur(80px);
  }

  .ai-luxury-hero__product-pulse-hearts-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 100%;
    height: 100%;
    pointer-events: none;
  }

  .ai-luxury-hero__pulse-heart-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    opacity: 0;
    pointer-events: none;
    filter: drop-shadow(0 0 20px {{ block.settings.pulse_heart_color }});
  }

  .ai-luxury-hero__product-wrapper-{{ ai_gen_id }}:hover .ai-luxury-hero__pulse-heart-{{ ai_gen_id }} {
    animation: ai-luxury-hero-pulse-out-{{ ai_gen_id }} {{ block.settings.pulse_duration }}s ease-out infinite;
  }

  @keyframes ai-luxury-hero-pulse-out-{{ ai_gen_id }} {
    0% {
      transform: translate(-50%, -50%) scale(0.3);
      opacity: {{ block.settings.pulse_start_opacity }};
      filter: drop-shadow(0 0 30px {{ block.settings.pulse_heart_color }});
    }
    50% {
      opacity: {{ block.settings.pulse_mid_opacity }};
      filter: drop-shadow(0 0 40px {{ block.settings.pulse_heart_color }});
    }
    100% {
      transform: translate(-50%, -50%) scale({{ block.settings.pulse_scale }});
      opacity: 0;
      filter: drop-shadow(0 0 50px {{ block.settings.pulse_heart_color }});
    }
  }

  .ai-luxury-hero__product-image-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    height: auto;
    display: block;
    z-index: 2;
    filter: drop-shadow(0 20px 40px rgba(0, 0, 0, 0.15));
  }

  .ai-luxury-hero__product-placeholder-{{ ai_gen_id }} {
    width: 100%;
    aspect-ratio: 1;
    background: rgba(255, 255, 255, 0.1);
    border-radius: 20px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-luxury-hero__product-placeholder-{{ ai_gen_id }} svg {
    width: 60%;
    height: 60%;
    opacity: 0.3;
  }

  .ai-luxury-hero__text-content-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 16px;
    max-width: 700px;
  }

  .ai-luxury-hero__headline-{{ ai_gen_id }} {
    font-family: {{ block.settings.headline_font.family }}, serif;
    font-weight: {{ block.settings.headline_font.weight }};
    font-size: {{ block.settings.headline_size }}px;
    line-height: 1.2;
    color: {{ block.settings.headline_color }};
    margin: 0;
    letter-spacing: -0.02em;
  }

  .ai-luxury-hero__subtext-{{ ai_gen_id }} {
    font-family: {{ block.settings.subtext_font.family }}, sans-serif;
    font-weight: {{ block.settings.subtext_font.weight }};
    font-size: {{ block.settings.subtext_size }}px;
    line-height: 1.6;
    color: {{ block.settings.subtext_color }};
    margin: 0;
    opacity: 0.9;
  }

  .ai-luxury-hero__cta-group-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 16px;
    align-items: center;
    margin-top: 20px;
  }

  .ai-luxury-hero__primary-cta-{{ ai_gen_id }} {
    position: relative;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    gap: 8px;
    padding: 18px 40px;
    background-color: {{ block.settings.primary_button_bg }};
    color: {{ block.settings.primary_button_text }};
    font-family: {{ block.settings.subtext_font.family }}, sans-serif;
    font-weight: 600;
    font-size: 16px;
    text-decoration: none;
    border-radius: 50px;
    border: none;
    cursor: pointer;
    overflow: hidden;
    transition: transform 0.3s ease, box-shadow 0.3s ease;
    box-shadow: 0 8px 24px {{ block.settings.primary_button_bg }}40;
  }

  .ai-luxury-hero__primary-cta-{{ ai_gen_id }}:hover {
    transform: translateY(-2px);
    box-shadow: 0 12px 32px {{ block.settings.primary_button_bg }}60;
  }

  .ai-luxury-hero__button-glow-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 100%;
    height: 100%;
    background: radial-gradient(circle, {{ block.settings.primary_button_bg }} 0%, transparent 70%);
    opacity: 0;
    transition: opacity 0.3s ease;
    pointer-events: none;
  }

  .ai-luxury-hero__primary-cta-{{ ai_gen_id }}:hover .ai-luxury-hero__button-glow-{{ ai_gen_id }} {
    opacity: 0.6;
  }

  .ai-luxury-hero__heart-icon-{{ ai_gen_id }} {
    width: 18px;
    height: 18px;
    fill: currentColor;
  }

  .ai-luxury-hero__primary-cta-{{ ai_gen_id }}:hover .ai-luxury-hero__heart-icon-{{ ai_gen_id }} {
    animation: ai-luxury-hero-heart-beat-{{ ai_gen_id }} 0.6s ease-in-out infinite;
  }

  @keyframes ai-luxury-hero-heart-beat-{{ ai_gen_id }} {
    0%, 100% {
      transform: scale(1);
    }
    25% {
      transform: scale(1.2);
    }
    50% {
      transform: scale(1);
    }
    75% {
      transform: scale(1.15);
    }
  }

  .ai-luxury-hero__secondary-cta-{{ ai_gen_id }} {
    display: inline-flex;
    align-items: center;
    gap: 6px;
    padding: 12px 24px;
    background: transparent;
    color: {{ block.settings.secondary_button_text }};
    font-family: {{ block.settings.subtext_font.family }}, sans-serif;
    font-weight: 500;
    font-size: 15px;
    text-decoration: none;
    border: 1px solid {{ block.settings.secondary_button_border }};
    border-radius: 50px;
    cursor: pointer;
    transition: background-color 0.3s ease, border-color 0.3s ease;
  }

  .ai-luxury-hero__secondary-cta-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.secondary_button_hover_bg }};
    border-color: {{ block.settings.secondary_button_hover_border }};
  }

  @media screen and (max-width: 749px) {
    .ai-luxury-hero__container-{{ ai_gen_id }} {
      padding: 60px 20px 40px;
      gap: 30px;
    }

    .ai-luxury-hero__content-{{ ai_gen_id }} {
      gap: 30px;
    }

    .ai-luxury-hero__product-wrapper-{{ ai_gen_id }} {
      max-width: 320px;
    }

    .ai-luxury-hero__headline-{{ ai_gen_id }} {
      font-size: {{ block.settings.headline_size | times: 0.6 }}px;
    }

    .ai-luxury-hero__subtext-{{ ai_gen_id }} {
      font-size: {{ block.settings.subtext_size | times: 0.9 }}px;
    }

    .ai-luxury-hero__cta-group-{{ ai_gen_id }} {
      width: 100%;
    }

    .ai-luxury-hero__primary-cta-{{ ai_gen_id }},
    .ai-luxury-hero__secondary-cta-{{ ai_gen_id }} {
      width: 100%;
      max-width: 320px;
    }
  }

  @media screen and (min-width: 750px) and (max-width: 989px) {
    .ai-luxury-hero__headline-{{ ai_gen_id }} {
      font-size: {{ block.settings.headline_size | times: 0.75 }}px;
    }

    .ai-luxury-hero__product-wrapper-{{ ai_gen_id }} {
      max-width: 400px;
    }
  }
{% endstyle %}

<luxury-hero-{{ ai_gen_id }} class="ai-luxury-hero-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-luxury-hero__hearts-canvas-{{ ai_gen_id }}"></div>
  
  <div class="ai-luxury-hero__container-{{ ai_gen_id }}">
    <div class="ai-luxury-hero__content-{{ ai_gen_id }}">
      
      <div class="ai-luxury-hero__product-wrapper-{{ ai_gen_id }}">
        <div class="ai-luxury-hero__product-glow-{{ ai_gen_id }}"></div>
        
        <div class="ai-luxury-hero__product-pulse-hearts-{{ ai_gen_id }}">
          {% for i in (1..5) %}
            {% assign delay = i | minus: 1 | times: 0.25 %}
            {% if i <= block.settings.pulse_heart_count %}
              <svg 
                class="ai-luxury-hero__pulse-heart-{{ ai_gen_id }}" 
                style="animation-delay: {{ delay }}s; width: {{ block.settings.pulse_heart_size }}px; height: {{ block.settings.pulse_heart_size }}px;"
                viewBox="0 0 24 24" 
                xmlns="http://www.w3.org/2000/svg"
              >
                <path 
                  d="M12 21.35l-1.45-1.32C5.4 15.36 2 12.28 2 8.5 2 5.42 4.42 3 7.5 3c1.74 0 3.41.81 4.5 2.09C13.09 3.81 14.76 3 16.5 3 19.58 3 22 5.42 22 8.5c0 3.78-3.4 6.86-8.55 11.54L12 21.35z" 
                  fill="{{ block.settings.pulse_heart_color }}"
                  stroke="{{ block.settings.pulse_heart_color }}"
                  stroke-width="2"
                />
              </svg>
            {% endif %}
          {% endfor %}
        </div>

        {% if block.settings.product_image %}
          <img
            src="{{ block.settings.product_image | image_url: width: 1000 }}"
            alt="{{ block.settings.product_image.alt | escape }}"
            class="ai-luxury-hero__product-image-{{ ai_gen_id }}"
            loading="eager"
            width="1000"
            height="1000"
          >
        {% else %}
          <div class="ai-luxury-hero__product-placeholder-{{ ai_gen_id }}">
            {{ 'product-1' | placeholder_svg_tag }}
          </div>
        {% endif %}
      </div>

      <div class="ai-luxury-hero__text-content-{{ ai_gen_id }}">
        {% if block.settings.headline != blank %}
          <h1 class="ai-luxury-hero__headline-{{ ai_gen_id }}">
            {{ block.settings.headline }}
          </h1>
        {% endif %}

        {% if block.settings.subtext != blank %}
          <p class="ai-luxury-hero__subtext-{{ ai_gen_id }}">
            {{ block.settings.subtext }}
          </p>
        {% endif %}
      </div>

      <div class="ai-luxury-hero__cta-group-{{ ai_gen_id }}">
        {% if block.settings.primary_cta_text != blank %}
          <a
            href="{{ block.settings.primary_cta_link }}"
            class="ai-luxury-hero__primary-cta-{{ ai_gen_id }}"
          >
            <span class="ai-luxury-hero__button-glow-{{ ai_gen_id }}"></span>
            <svg class="ai-luxury-hero__heart-icon-{{ ai_gen_id }}" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
              <path d="M12 21.35l-1.45-1.32C5.4 15.36 2 12.28 2 8.5 2 5.42 4.42 3 7.5 3c1.74 0 3.41.81 4.5 2.09C13.09 3.81 14.76 3 16.5 3 19.58 3 22 5.42 22 8.5c0 3.78-3.4 6.86-8.55 11.54L12 21.35z"/>
            </svg>
            {{ block.settings.primary_cta_text }}
          </a>
        {% endif %}

        {% if block.settings.secondary_cta_text != blank %}
          <button
            class="ai-luxury-hero__secondary-cta-{{ ai_gen_id }}"
            data-scroll-target="{{ block.settings.secondary_cta_scroll_id }}"
          >
            {{ block.settings.secondary_cta_text }}
          </button>
        {% endif %}
      </div>

    </div>
  </div>
</luxury-hero-{{ ai_gen_id }}>

<script>
(function() {
  class LuxuryHero{{ ai_gen_id }} extends HTMLElement {
    constructor() {
      super();
      this.heartsCanvas = this.querySelector('.ai-luxury-hero__hearts-canvas-{{ ai_gen_id }}');
      this.container = this.querySelector('.ai-luxury-hero__container-{{ ai_gen_id }}');
      this.productWrapper = this.querySelector('.ai-luxury-hero__product-wrapper-{{ ai_gen_id }}');
      this.hearts = [];
      this.heartCount = {{ block.settings.heart_count }};
      this.scrollY = 0;
      this.rafId = null;
    }

    connectedCallback() {
      this.createHearts();
      this.setupScrollParallax();
      this.setupSecondaryButton();
      this.animateHearts();
    }

    disconnectedCallback() {
      if (this.rafId) {
        cancelAnimationFrame(this.rafId);
      }
      window.removeEventListener('scroll', this.handleScroll);
    }

    createHearts() {
      const heartSVG = '<svg viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path d="M12 21.35l-1.45-1.32C5.4 15.36 2 12.28 2 8.5 2 5.42 4.42 3 7.5 3c1.74 0 3.41.81 4.5 2.09C13.09 3.81 14.76 3 16.5 3 19.58 3 22 5.42 22 8.5c0 3.78-3.4 6.86-8.55 11.54L12 21.35z" fill="{{ block.settings.heart_color }}"/></svg>';
      
      for (let i = 0; i < this.heartCount; i++) {
        const heart = document.createElement('div');
        heart.className = 'ai-luxury-hero__heart-{{ ai_gen_id }}';
        heart.innerHTML = heartSVG;
        
        const size = Math.random() * 30 + 25;
        heart.style.width = size + 'px';
        heart.style.height = size + 'px';
        heart.style.left = Math.random() * 100 + '%';
        heart.style.bottom = '-50px';
        
        const data = {
          element: heart,
          x: parseFloat(heart.style.left),
          y: -50,
          speed: Math.random() * 0.3 + 0.15,
          drift: (Math.random() - 0.5) * 0.3,
          delay: Math.random() * 5000,
          opacity: Math.random() * 0.2 + 0.08,
          startTime: Date.now() + Math.random() * 5000
        };
        
        this.hearts.push(data);
        this.heartsCanvas.appendChild(heart);
      }
    }

    animateHearts() {
      const now = Date.now();
      
      this.hearts.forEach(heart => {
        if (now < heart.startTime) {
          return;
        }
        
        heart.y += heart.speed;
        heart.x += heart.drift;
        
        if (heart.y > 110) {
          heart.y = -10;
          heart.x = Math.random() * 100;
          heart.startTime = now + Math.random() * 3000;
        }
        
        if (heart.x < -5) heart.x = 105;
        if (heart.x > 105) heart.x = -5;
        
        heart.element.style.transform = 'translate(' + heart.x + 'vw, ' + heart.y + 'vh)';
        heart.element.style.opacity = heart.opacity;
      });
      
      this.rafId = requestAnimationFrame(() => this.animateHearts());
    }

    setupScrollParallax() {
      this.handleScroll = () => {
        this.scrollY = window.pageYOffset;
        if (this.container) {
          const parallaxAmount = this.scrollY * {{ block.settings.parallax_speed }};
          this.container.style.transform = 'translateY(' + parallaxAmount + 'px)';
        }
      };
      
      window.addEventListener('scroll', this.handleScroll, { passive: true });
    }

    setupSecondaryButton() {
      const secondaryBtn = this.querySelector('.ai-luxury-hero__secondary-cta-{{ ai_gen_id }}');
      if (secondaryBtn) {
        secondaryBtn.addEventListener('click', function() {
          const targetId = this.getAttribute('data-scroll-target');
          if (targetId) {
            const targetElement = document.getElementById(targetId);
            if (targetElement) {
              targetElement.scrollIntoView({ behavior: 'smooth', block: 'start' });
            }
          }
        });
      }
    }
  }

  customElements.define('luxury-hero-{{ ai_gen_id }}', LuxuryHero{{ ai_gen_id }});
})();
</script>

